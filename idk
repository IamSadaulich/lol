for i, v in pairs(game:GetService("ReplicatedStorage").identifierStorage:GetChildren()) do
    local value = v.Value -- v is the child of object
	local value1 = value
wait(0.07)
local args = {
    [1] = {
        [1] = {
            [1] = "Daily",
            [2] = "Summon10Units"
        },
        [2] = value1
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("dataRemoteEvent"):FireServer(unpack(args))
